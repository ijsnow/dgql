    "@zeit/next-css": "^1.0.1",
    "autoprefixer": "^9.6.5",
    "ava": "^2.4.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.0",
