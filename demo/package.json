{
  "name": "@jago-eng/timely",
  "version": "0.0.1",
  "description": "A platform for managing events.",
  "main": "index.js",
  "engines": "12.x",
  "license": "MIT",
  "private": true,
  "scripts": {
    "generate": "concurrently \"yarn generate:api\" \"yarn generate:client\"",
    "generate:api": "go run github.com/99designs/gqlgen",
    "generate:client": "yarn graphql-codegen --config ./types/gen.yml",
    "postgenerate:client": "yarn format",
    "format": "eslint --fix '**/*.{ts,tsx}'",
    "build": "next build",
    "start": "next start",
    "dev": "next dev",
    "test": "ava",
    "storybook": "start-storybook -p 6006 -s ./public",
    "sb": "yarn storybook",
    "initialize": "ts-node --project tsconfig.node.json -r tsconfig-paths/register \"./lifecycle/initialize.ts\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.5",
    "@jago-eng/tsconfig-server": "^0.0.5",
    "@now/node": "^1.2.0",
    "@restart/hooks": "^0.3.15",
    "@types/lodash": "^4.14.149",
    "apollo-server": "^2.9.12",
    "apollo-server-cache-redis": "^1.1.4",
    "apollo-server-micro": "^2.9.12",
    "apollo-server-testing": "^2.9.12",
    "axios": "^0.21.1",
    "dgraph-js": "^2.0.2",
    "graphql": "^14.5.8",
    "graphql-request": "^1.8.2",
    "graphql-tools": "^4.0.6",
    "grpc": "^1.24.2",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "namor": "^2.0.2",
    "next": "^9.1.3",
    "nexus": "^0.12.0-rc.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "swr": "^0.1.12"
  },
  "eslintConfig": {
    "extends": [
      "@jago-eng/eslint-config"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@fullhuman/postcss-purgecss": "^1.3.0",
